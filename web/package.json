{
  "name": "@aurora/web",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "WEBPACK_TARGET=all rspack build --config webpack.config.js --mode production",
    "build:web": "WEBPACK_TARGET=web rspack build --config webpack.config.js --mode production",
    "build:electron": "WEBPACK_TARGET=electron rspack build --config webpack.config.js --mode production",
    "dev": "WEBPACK_TARGET=web rspack dev --config webpack.config.js --mode development",
    "start": "pnpm build:electron && electron dist/electron/electron-main.js",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@swc/helpers": "^0.5.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.0",
    "electron": "^28.1.4",
    "lucide-react": "^0.312.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rspack/cli": "^0.5.1",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.0.0",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "webpack-merge": "^5.10.0"
  },
  "packageManager": "pnpm@8.14.1"
}
